faucet 0xA 100
faucet 0xB 0
deploy 0xA:0xC("0xB", 1000) "test/contracts/Vault.sol"

0xA:0xC.receive{value:50}()
0xA:0xC.withdraw("0xZ", 20)
assert 0xA this.balance==50
assert 0xC this.balance==50
assert 0xZ this.balance==0

revert 0xA:0xC.finalize()
block.number = 10001

0xB:0xC.cancel()

revert 0xA:0xC.finalize()
assert 0xC this.balance==50
