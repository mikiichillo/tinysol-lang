faucet 0xA 100
deploy 0xC1 "test/c1.sol"
faucet 0xC1 10
assert 0xC1 balance 10
0xA:0xC1.f1()
assert 0xC1 x 0
assert 0xC1 b true
0xA:0xC1.f1{value : 1}()
assert 0xC1 x 1
assert 0xC1 b true
0xA:0xC1.f2("0xA")
assert 0xC1 balance 9
assert 0xA balance 101
0xA:0xC1.f3(3)
assert 0xC1 balance 6
assert 0xC1 b true
assert 0xA balance 104
0xA:0xC1.f3(3)
assert 0xC1 b false
assert 0xA balance 104